# ex_project
package interfaceex;

public class CalculatorCalc extends Calculator{

	@Override
	public int time(int num1, int num2) {
		return num1*num2;
	}

	@Override
	public int divide(int num1, int num2) {
		if(num2 != 0)
			return num1/num2;
		else
			return Calc.ERROR;
	}
	
	public int square(int num1) {
		return num1*num1;
	}
	
	public void showInfo() {
		System.out.println("인터페이스 구현");
	}

}
