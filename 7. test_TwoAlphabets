package seven_test;

public class TwoAlphabets {

	public static void main(String[] args) {
		/*char[][] alphabets = {{'a','b'}, {'c','d'},{'e','f'}};
		
		for(int i=0; i<alphabets.length; i++) {
			for(int j=0; j<alphabets[i].length; j++) {
				System.out.println(alphabets[i][j]);
			}
			System.out.println();
		} */
		
		char[][] alphabetsArr = new char[13][2];
		char ch = 'a';
		
		for(int i = 0; i<alphabetsArr.length; i++) {
			for(int j = 0; j < alphabetsArr[i].length; j++) {
				alphabetsArr[i][j] = ch;
				System.out.print(alphabetsArr[i][j]+ "  ");
				ch++;
			}
			System.out.println();
		}

	}

}
