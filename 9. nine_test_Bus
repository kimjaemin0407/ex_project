# ex_project
package nine_test;

public class Bus extends Car{

	@Override
	public void run() {
		System.out.println("버스가 달린다");
		
	}

	@Override
	public void refuel() {
		System.out.println("천연가스를 충전");
		
	}

	@Override
	public void stop() {
		System.out.println("차가 멈춥니다");
		
	}
	
	public void takePassenger() {
		System.out.println("승객을 버스에 태운다");
	}

}
