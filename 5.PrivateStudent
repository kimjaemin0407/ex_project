# ex_project
package hiding;

public class PrivateStudent {
	int studentID;
	private String studentName;
	int grade;
	String address;        //멤버 변수
	
	
	public String getStudentName() {  //메서드
		return studentName;          
	}
	
	public void setStudentName(String name) {  //메서드
		studentName = name;
	}
}
