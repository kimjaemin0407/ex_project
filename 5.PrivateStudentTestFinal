package hiding;

public class PrivateStudentTestFinal {

	public static void main(String[] args) {
		PrivateStudent Studentkim = new PrivateStudent();
		//Studentkim.studentName = "김재민";  // PrivateStudent의 Private String name때문에 오류가 난다
		
