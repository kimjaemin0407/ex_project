# ex_project
package test;

public class Person {
	public String Name;
	public int money;
	
	public Person(String Name, int money) {
		this.Name = Name;
		this.money = money;
	}
	
	public void takeStar(StarCoffee star) {
		star.take(4000);
		this.money -= 4000;
	}
	
	public void takeBeen(BeenCoffee been) {
		been.take(4500);
		this.money -= 4500;
	}
	
	public void showInfo() {
		System.out.println("남은 돈은" + money + "입니다.");
	}

}
