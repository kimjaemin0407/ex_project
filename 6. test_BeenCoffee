# ex_project
package test;

public class BeenCoffee {
	public String BeenCoffeeName;
	public int money;
	
	public BeenCoffee(String BeenCoffeeName) {
		this.BeenCoffeeName = BeenCoffeeName;
	}
	
	public void take(int money) {
		this.money += money;
	}
	
	public void showInfo() {
		System.out.println("팔린 커피는 " + BeenCoffeeName + "이고, 총 수입은" + money + "입니다.");
	}

}
