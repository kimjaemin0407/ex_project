# ex_project
package chapter2;

public class CharacterEx1 {

	public static void main(String[] args) {
		char ch1 = 'A';
		System.out.println(ch1);
		System.out.println((int)ch1); // 문자에 해당하는 아스키 코드 값 출력
		
		char ch2 = 66;
		System.out.println(ch2); // 아스키코드 값에 해당하는 문자 출력
		
		int ch3 = 67;
		System.out.println(ch3);
		System.out.println((char)ch3); // 아스키코드 값에 해당하는 문자 출력

	}

}
