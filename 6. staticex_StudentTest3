# ex_project
package staticex;

public class StudentTest3 {

	public static void main(String[] args) {
		Student2 studentkim = new Student2();
		studentkim.setStudentName("김재민");
		System.out.println(Student2.getSerialNum()); 
		System.out.println(studentkim.studentName + studentkim.serialNum);
		
		Student2 studentsu = new Student2();
		studentsu.setStudentName("수");
		System.out.println(studentsu.getSerialNum());
		System.out.println(studentsu.studentName + studentsu.serialNum);

	}

}
// serialNum을 직접 참조하지 않고 getSerialNum() 메서드를 호출하여 참조, 인스턴스 참조변수가 아닌 클래스 이름으로 직접 호출 할 수 있다.
