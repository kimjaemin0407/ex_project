# ex_project
package array;

public class BookArray2 {

	public static void main(String[] args) {
			Book[] library = new Book[5];
			
			library[0] = new Book("자바", "김재민");
			library[1] = new Book("자바1", "김재민1");
			library[2] = new Book("자바2", "김재민2");
			library[3] = new Book("자바3", "김재민3");
			library[4] = new Book("자바4", "김재민4");
			
			for(int i = 0; i < library.length; i++) {
				library[i].showInfo();
			}
			
			for(int i = 0; i < library.length; i++) {
				System.out.println(library[i]);
			}
	}
}
