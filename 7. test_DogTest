# ex_project
package seven_test;

public class DogTest {

	public static void main(String[] args) {
		Dog[] dog = new Dog[5];
		
		dog[0] = new Dog("A", "AA");
		dog[1] = new Dog("B", "BB");
		dog[2] = new Dog("C", "CC");
		dog[3] = new Dog("D", "DD");
		dog[4] = new Dog("E", "EE");
		
		for(int i = 0; i<dog.length; i++) {
			System.out.println(dog[i].showInfo());
		}
		
		for(Dog D : dog) {
			System.out.println(D.showInfo());
		}

	}

}
