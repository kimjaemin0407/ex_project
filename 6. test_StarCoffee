# ex_project
package test;

public class StarCoffee {
	public String StarCoffeeName;
	public int money;
	
	public StarCoffee(String StarCoffeeName) {
		this.StarCoffeeName = StarCoffeeName;
	}
	
	public void take(int money) {
		this.money += money;
	}
	
	public void showInfo() {
		System.out.println("팔린 커피는 " + StarCoffeeName + "이고, 총 수입은" + money + "입니다.");
	}

}
